{
  "name": "mykytakotenko/acmewidgetco",
  "description": "Test Task Shopping Basket CLI",
  "type": "project",
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "brick/money": "^0.9.0",
    "psr/container": "^2.0",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-phpunit": "^1.3",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "AcmeWidgetCo\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AcmeWidgetCo\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  },
  "config": {
    "sort-packages": true
  }
}
